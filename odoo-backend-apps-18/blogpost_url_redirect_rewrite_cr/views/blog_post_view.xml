<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="view_blog_post_form_extended" model="ir.ui.view">
            <field name="name">Blog Post URL Added</field>
            <field name="model">blog.post</field>
            <field name="inherit_id" ref="website_blog.view_blog_post_form"/>
            <field name="arch" type="xml">
                <field name="website_meta_keywords" position="after">
                        <field name="website_url_latest"/>
                        <field name="destination_url"/>
                </field>
            </field>
        </record>

    </data>
</odoo>
