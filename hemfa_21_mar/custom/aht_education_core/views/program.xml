<odoo>
    <data>
        <record model="ir.ui.view" id="program_title_list_view">
            <field name="name">Program Title List</field>
            <field name="model">program.title</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="name"/>
                </tree>
            </field>
        </record>
        <record model="ir.ui.view" id="program_title_form_view">
            <field name="name">Program Title form</field>
            <field name="model">program.title</field>
            <field name="arch" type="xml">
                <form>
                    <header>
                    </header>
                    <sheet>
                        <separator style="color:green;font-size:18px;" string="Program Title" colspan="4"/>
                        <group>
                            <field name="name"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
        <record model="ir.actions.act_window" id="program_title_action_window">
            <field name="name">Program Title</field>
            <field name="res_model">program.title</field>
            <field name="type">ir.actions.act_window</field>
            <field name="view_mode">tree,form</field>
        </record>
  
  
        <!-- program level -->
