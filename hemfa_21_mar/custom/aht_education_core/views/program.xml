        <record model="ir.ui.view" id="aht_program_list_view">
            <field name="name">Program List</field>
            <field name="model">aht.program</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="complete_name"/>
                    <field name="department_id"/>
                    <field name="program_level_id"/>
                    <field name="degree_abbr_id"/>
                    <field name="program_title_id"/>
                    
                    <field name="program_code"/>
                    <field name="program_depends_id"/>
                    <field name="program_duration_id"/>
                </tree>
            </field>
        </record>
        <record model="ir.ui.view" id="aht_program_form_view">
            <field name="name">Program form</field>
            <field name="model">aht.program</field>
            <field name="arch" type="xml">
                <form>
                    <header>
                    </header>
                    <sheet>
                        <separator style="color:green;font-size:18px;" string="Program" colspan="4"/>
                        <group>
                            <group>
                                <field name="department_id"/>
                                <field name="program_level_id"/>
                               <field name="degree_abbr_id"/>
                            </group>
                            
                            <group>
                                <field name="program_title_id"/>
                                <field name="program_code"/>
                                <field name="program_depends_id"/>
